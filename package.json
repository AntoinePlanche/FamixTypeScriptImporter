{
  "name": "ts2famix",
  "version": "0.1.0",
  "description": "Examples of the TypeScript compiler API usage",
  "main": "dist/ts2famix-cli.js",
  "scripts": {
    "dev": "ts-node src/ts2famix-cli.ts",
    "debug": "node --inspect-brk node_modules/.bin/ts-node",
    "build": "tsc",
    "test": "jest --colors --silent",
    "local": "sudo npm i -g && ts2famix",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bin": {
    "ts2famix": "dist/ts2famix-cli.js"
  },
  "keywords": [],
  "author": "",
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.10.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "ts-morph": "^12.2.0",
    "tsutils": "^3.21.0",
    "yargs": "^17.4.0"
  }
}
